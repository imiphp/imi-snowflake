{
    "name": "imiphp/imi-snowflake",
    "type": "library",
    "license": "MIT",
    "description": "imi 框架的雪花算法生成组件",
    "require": {
        "godruoyi/php-snowflake": "^1.0"
    },
    "require-dev": {
        "yurunsoft/imi": "dev-dev",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4" : {
            "Imi\\Snowflake\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Imi\\Snowflake\\Test\\" : "tests/"
        }
    },
    "scripts": {
      "test": "./tests/run",
      "install-test": "php --ri swoole && composer install composer test"
    }
}
